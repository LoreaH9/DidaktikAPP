<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view3CL"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/style_redondeado">

    <ImageView
        android:id="@+id/pieza1IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pieza1" />

    <ImageView
        android:id="@+id/pieza2IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginStart="60dp"
        app:layout_constraintBottom_toTopOf="@+id/pieza3IV"
        app:layout_constraintStart_toStartOf="@+id/pieza3IV"
        app:layout_constraintTop_toBottomOf="@+id/pieza1IV"
        app:srcCompat="@drawable/pieza2" />

    <ImageView
        android:id="@+id/pieza3IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pieza3" />

    <ImageView
        android:id="@+id/pieza4IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pieza4" />

    <ImageView
        android:id="@+id/pieza5IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toTopOf="@+id/pieza6IV"
        app:layout_constraintEnd_toEndOf="@+id/pieza6IV"
        app:layout_constraintStart_toEndOf="@+id/constraintLayout"
        app:layout_constraintTop_toBottomOf="@+id/pieza4IV"
        app:srcCompat="@drawable/pieza5" />

    <ImageView
        android:id="@+id/pieza6IV"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/pieza6" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="202dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/hueco1IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:alpha="0.65"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/pieza1" />

        <ImageView
            android:id="@+id/hueco2IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:alpha="0.65"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/hueco1IV"
            app:srcCompat="@drawable/pieza2" />

        <ImageView
            android:id="@+id/hueco3IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginTop="2dp"
            android:alpha="0.65"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hueco1IV"
            app:srcCompat="@drawable/pieza3" />

        <ImageView
            android:id="@+id/hueco4IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:alpha="0.65"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/hueco3IV"
            app:srcCompat="@drawable/pieza4" />

        <ImageView
            android:id="@+id/hueco5IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:layout_marginTop="2dp"
            android:alpha="0.65"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hueco3IV"
            app:srcCompat="@drawable/pieza5" />

        <ImageView
            android:id="@+id/hueco6IV"
            android:layout_width="100dp"
            android:layout_height="90dp"
            android:alpha="0.65"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/hueco5IV"
            app:srcCompat="@drawable/pieza6" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>