                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/lugar_7"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/foto7_8"
                app:layout_constraintEnd_toEndOf="@+id/text7_9"
                app:layout_constraintTop_toTopOf="@+id/foto7_8" />

            <TextView
                android:id="@+id/text7_8"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:background="@drawable/style_redondeado_editext"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/lugar_8"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/foto7_9"
                app:layout_constraintEnd_toEndOf="@+id/text7_7"
                app:layout_constraintTop_toTopOf="@+id/foto7_9" />

            <TextView
                android:id="@+id/statusTextView"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:textColor="@color/negro"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

