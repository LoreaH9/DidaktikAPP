                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/lugar_5"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/foto7_3"
                app:layout_constraintEnd_toEndOf="@+id/text7_1"
                app:layout_constraintTop_toTopOf="@+id/foto7_3" />

            <TextView
                android:id="@+id/text7_2"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:background="@drawable/style_redondeado_editext"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/lugar_2"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/foto7_4"
                app:layout_constraintEnd_toEndOf="@+id/text7_5"
                app:layout_constraintTop_toTopOf="@+id/foto7_4" />

            <TextView
                android:id="@+id/text7_6"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:background="@drawable/style_redondeado_editext"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="10dp"
                android:text="@string/lugar_6"
                android:textAlignment="center"
                android:textColor="@color/negro"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/foto7_5"
